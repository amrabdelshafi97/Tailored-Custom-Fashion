{
  "manifest_version": 3,
  "name": "Virtual Fitting Room",
  "version": "0.1",

  "action": {
    "default_icon": {
      "32": "images/icon.png"
    },
    "default_title": "Virtual Fitting Room",
    "default_popup": "index.html"
  },
  "description": "Tailored Fashion Experience: Personalized Visualizations for Customers",
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
